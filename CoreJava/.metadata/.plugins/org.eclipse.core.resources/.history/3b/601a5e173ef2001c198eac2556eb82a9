		Scanner sc = new Scanner(System.in);
		long lpa = sc.nextLong();
		if (lpa >= 0 && lpa <= 180000)
			System.out.println("No tax required");
		else if (lpa >= 180001 && lpa <= 300000)
			System.out.println("payable tax is 10% of lpa");
		else if (lpa >= 300001 && lpa <= 500000)
			System.out.println("payable tax is 20% of lpa");
		else if (lpa >= 500001 && lpa <= 1000000)
			System.out.println("payable tax is 30% of lpa");
			


